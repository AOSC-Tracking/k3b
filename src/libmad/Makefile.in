	@list='$(include_HEADERS)'; for p in $$list; do \
	if test -f "$$p"; then d= ; else d="$(srcdir)/"; fi; \
	echo " $(INSTALL_DATA) $$d$$p $(DESTDIR)$(includedir)/$$p"; \
	$(INSTALL_DATA) $$d$$p $(DESTDIR)$(includedir)/$$p; \
	done

